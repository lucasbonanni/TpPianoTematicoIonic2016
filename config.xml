<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.piano745356" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>piano</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="you@example.com" href="http://example.com.com/">
      Your Name Here
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,root" />
  <preference name="AndroidPersistentFileLocation" value="Compatibility" />
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="SplashScreenDelay" value="2000"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="cordova-plugin-device" spec="~1.1.2"/>
  <plugin name="cordova-plugin-console" spec="~1.0.3"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.2.2"/>
  <plugin name="cordova-plugin-splashscreen" spec="~3.2.2"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.1.3"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <platform name="android">
    <!--
        ldpi    : 36x36 px
        mdpi    : 48x48 px
        hdpi    : 72x72 px
        xhdpi   : 96x96 px
        xxhdpi  : 144x144 px
        xxxhdpi : 192x192 px
    -->
    <icon src="resources/android/icon/54734.png" width="57" height="57"/>
    <icon src="resources/android/icon/54734.png" width="114" height="114"/>
    <icon src="resources/android/icon/54734.png" width="40" height="40"/>
    <icon src="resources/android/icon/54734.png" width="80" height="80"/>
    <icon src="resources/android/icon/54734.png" width="50" height="50"/>
    <icon src="resources/android/icon/54734.png" width="100" height="100"/>
    <icon src="resources/android/icon/54734.png" width="60" height="60"/>
    <icon src="resources/android/icon/54734.png" width="120" height="120"/>
    <icon src="resources/android/icon/54734.png" width="180" height="180"/>
    <icon src="resources/android/icon/54734.png" width="72" height="72"/>
    <icon src="resources/android/icon/54734.png" width="144" height="144"/>
    <icon src="resources/android/icon/54734.png" width="76" height="76"/>
    <icon src="resources/android/icon/54734.png" width="152" height="152"/>
    <icon src="resources/android/icon/54734.png" width="29" height="29"/>
    <icon src="resources/android/icon/54734.png" width="58" height="58"/>
    <splash src="resources/android/splash/drawable-ldpi/background.2.png" />
    <!-- density="ldpi" -->
    <!--splash src="resources/android/splash/drawable-mdpi/background.3.png" density="mdpi" />
    <splash src="resources/android/splash/drawable-hdpi/background.1.png" density="hdpi" />
    <splash src="resources/android/splash/drawable-xhdpi/background.4.png" density="xhdpi" />
    <splash src="resources/android/splash/drawable-xxhdpi/background.5.png" density="xxhdpi" />
    <splash src="resources/android/splash/drawable-xxxhdpi/background.6.png" density="xxxhdpi" /-->
</platform>
</widget>